syntax = "proto3";

package pb;

service Provision {
	rpc GetStatus (StatusReq) returns (StatusResp) {}
	rpc Apply (ApplyReq) retruns (ApplyResp) {}
	rpc Destroy (ApplyReq) retruns (ApplyResp) {}
}

message StatusReq {
	string project_id = 1;
}

message StatusResp {
	string project_id = 1;
	string status = 2;
}

message ApplyReq {
	string project_id = 1;
	string giturl     = 2;
	boolean isdestroy = 3;
}
